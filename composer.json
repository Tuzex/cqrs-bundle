{
  "name": "tuzex/cqrs-messenger-bundle",
  "description": "Implementation of CQRS (Command Query Responsibility Segregation) based on the Symfony Messenger for Symfony 4.3 and higher",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Barnas",
      "email": "iam@edobarnas.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Tuzex\\Bundle\\Cqrs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tuzex\\Bundle\\Cqrs\\Test\\": "tests"
    }
  },
  "require": {
    "php": "^7.4",
    "symfony/framework-bundle": "4.3.*|4.4.*|5.*",
    "tuzex/cqrs": "1.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "0.12.*",
    "phpstan/phpstan-phpunit": "0.12.*",
    "phpunit/phpunit": "9.4.*",
    "symplify/easy-coding-standard": "8.3.*"
  }
}
