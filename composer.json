{
  "name": "tuzex/cqrs-bundle",
  "description": "Integration of CQRS (Command Query Responsibility Segregation) based on the Symfony Messenger to Symfony 4.3 and higher",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Barnas",
      "email": "iam@edobarnas.com"
    }
  ],
  "config": {
    "platform": {
      "php": "8.0.1",
      "ext-intl": "1"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Tuzex\\Bundle\\Cqrs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tuzex\\Bundle\\Cqrs\\Test\\": "tests"
    }
  },
  "require": {
    "php": "8.*",
    "symfony/framework-bundle": "4.3.*|4.4.*|5.*",
    "tuzex/cqrs": "2.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "0.12.*",
    "phpstan/phpstan-phpunit": "0.12.*",
    "phpunit/phpunit": "9.5.*",
    "symplify/easy-coding-standard": "9.1.*"
  }
}
